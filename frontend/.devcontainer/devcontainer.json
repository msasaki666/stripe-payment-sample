{
  "name": "frontend",
  "dockerComposeFile": [
    "../../compose.yaml"
  ],
  "service": "frontend",
  "workspaceFolder": "/app/frontend",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.formatOnSave": true,
        "dotfiles.repository": "msasaki666/dotfiles",
        "dotfiles.targetPath": "~/dotfiles",
        "dotfiles.installCommand": "~/dotfiles/install.sh"
      },
      "extensions": [
        "mosapride.zenkaku",
        "mhutchie.git-graph",
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "ms-azuretools.vscode-docker",
        "github.copilot",
        "github.copilot-chat",
        "github.copilot-labs"
      ]
    }
  }
}
